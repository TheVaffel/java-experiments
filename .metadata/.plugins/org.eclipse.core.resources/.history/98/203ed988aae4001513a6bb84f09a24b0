package priv.hkon.theseq.blocks;

import priv.hkon.theseq.structures.Structure;
import priv.hkon.theseq.world.Village;

public class SubBlock extends Block { // A class with no own data, only to support superclass

	public SubBlock(int nx, int ny, Structure s, Village v) {
		super(nx, ny, s, v);
		
	}

	@Override
	public void makeData() {
		// TODO Auto-generated method stub

	}
	
	public int[][] getData(){
		return s.getData(nx - s.getX(), ny - s.getY());
	}

}
