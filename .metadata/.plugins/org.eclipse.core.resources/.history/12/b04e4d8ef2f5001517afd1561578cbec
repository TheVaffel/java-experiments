package priv.hkon.theseq.filters;

public class CombinedFilter extends Filter {
	
	Filter[] fs;

	public CombinedFilter(Filter f1, Filter f2) {
		fs = new Filter[2];
		this.f1 = f1;
		this.f2 = f2;
	}
	
	public CombinedFilter(Filter f1, Filter f2, Filter f3){
		
	}
	
	public void apply(int[] data){
		f1.apply(data);
		f2.apply(data);
	}
	
	

}
