	public Crate(int nx, int ny, Structure s, Village v) {
		super(nx, ny, s, v);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void makeData() {
		int offy = H/3;
		for(int i = offy; i < H; i++){
			if( i < offy + Tile.HEIGHT){
				
			}
		}

	}

	@Override
	public String getName() {
		return crate;
	}

}
